{
	"geode": "4.10.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "xblazegmd.cbf-indicator",
	"name": "CBF Indicator",
	"version": "v1.0.1",
	"developer": "XblazeGMD",
	"description": "Make it very obvious when someone's using CBF",
	"dependencies": {
		"geode.node-ids": ">=v1.20.0",
		"syzzi.click_between_frames": ">=1.4.6"
	},
	"resources": {
		"sprites": [
			"resources/cbf.png"
		]
	},
	"settings": {
		"indicator-section": {
			"type": "title",
			"name": "Indicator Options"
		},
		"show-as-image": {
			"type": "bool",
			"name": "Show Indicator As Image",
			"description": "If the CBF indicator should be an image (CBF Logo)",
			"default": true
		},
		"alignment": {
			"type": "string",
			"name": "Indicator Alignment",
			"one-of": ["Top-Left", "Top-Right", "Bottom-Left", "Bottom-Right"],
			"default": "Top-Left"
		},
		"endscreen-section": {
			"type": "title",
			"name": "Endscreen Options"
		},
		"lvl-complete-string": {
			"type": "string",
			"name": "Level Complete With CBF Text",
			"description": "The text that will appear when beating a level with CBF",
			"default": "CBF Detected Loser!",
			"filter": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ-_\"'%&$#!()"
		}
	},
	"tags": ["interface", "enhancement"],
	"links": {
		"source": "https://github.com/xblazegmd/cbf-indicator"
	}
}
